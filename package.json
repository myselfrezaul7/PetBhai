{
  "name": "petbhai",
  "homepage": "https://myselfrezaul7.github.io/PetBhai/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "dev": "vite",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "cd backend && npm run dev",
    "install:all": "npm install && cd backend && npm install",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "lint": "eslint --max-warnings=0 --no-warn-ignored",
    "lint:all": "eslint \"**/*.{ts,tsx,js,jsx}\" --max-warnings=0 --no-warn-ignored",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md,css,html}'",
    "check:html": "html-validate \"dist/**/*.html\" \"public/**/*.html\"",
    "ci": "npm run lint && npm run typecheck && npm run build && npm run check:html",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@google/genai": "latest",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^12.7.0",
    "helmet": "^7.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.22.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.26",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "html-validate": "^10.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^13.0.1",
    "prettier": "^3.7.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.50.1",
    "vite": "^5.2.0"
  }
}
